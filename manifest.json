{
  "manifest_version": 2,
  "name": "Bandaid",
  "version": "1.0",

  "description": "Points out likely-AI-generated text from webpages",

  "icons": {
    "48": "icons/main.png"
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["rules/emdash.js","rules/rule_of_three.js","rules/sentence_structures.js","rules/overused_words.js", "./bandaid.js"]
    }
  ]
}

